{
  "name": "hbpin-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier --cache --write .",
    "lint": "eslint --cache . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
